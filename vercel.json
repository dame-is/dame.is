{
    "buildCommand": "npm run build",
    "outputDirectory": "_site",
    "rewrites": [
      { "source": "/blog/", "destination": "/blog.html" },
      { "source": "/blog", "destination": "/blog.html" },
      { "source": "/blog/:slug", "destination": "/blog/:slug.html" },
      { "source": "/about", "destination": "/about.html" },
      { "source": "/ethos", "destination": "/ethos.html" },
      { "source": "/log", "destination": "/log.html" },
      { "source": "/patrons", "destination": "/patrons.html" },
      { "source": "/index", "destination": "/" },
      { "source": "/home", "destination": "/" }
    ],
    "redirects": [
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "skeet.tools"
          }
        ],
        "destination": "https://cred.blue/resources",
        "permanent": true
      },
      {
        "source": "/ratingalttext",
        "destination": "https://cred.blue/alt-text",
        "permanent": true
      },
      {
        "source": "/skeet-tools",
        "destination": "https://cred.blue/resources",
        "permanent": true
      }
    ]
  }