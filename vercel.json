{
    "buildCommand": "npm run build",
    "outputDirectory": "_site",
    "rewrites": [
      { "source": "/blog", "destination": "/blog/" },
      { "source": "/about", "destination": "/about/" },
      { "source": "/ethos", "destination": "/ethos/" },
      { "source": "/log", "destination": "/log/" },
      { "source": "/patrons", "destination": "/patrons/" },
      { "source": "/index", "destination": "/" },
      { "source": "/home", "destination": "/" },
      { "source": "/:path*", "destination": "https://publish.obsidian.md/serve?url=dame.is/:path*" }
    ],
    "redirects": [
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "skeet.tools"
          }
        ],
        "destination": "https://cred.blue/resources",
        "permanent": true
      },
      {
        "source": "/ratingalttext",
        "destination": "https://cred.blue/alt-text",
        "permanent": true
      },
      {
        "source": "/skeet-tools",
        "destination": "https://cred.blue/resources",
        "permanent": true
      }
    ]
  }