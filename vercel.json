{
    "rewrites": [
        { "source": "/blog/", "destination": "/blog.html" },
        { "source": "/blog", "destination": "/blog.html" },
        { "source": "/blog/:slug", "destination": "/blog-post.html?slug=:slug" },
        { "source": "/blog-post", "destination": "/blog-post.html" },
        { "source": "/about", "destination": "/about.html" },
        { "source": "/ethos", "destination": "/ethos.html" },
        { "source": "/log", "destination": "/log.html" },
        { "source": "/ratingmyalttext", "destination": "/ratingmyalttext.html" },
        { "source": "/skeet-tools", "destination": "/skeet-tools.html" },
        { "source": "/index", "destination": "/" },
        { "source": "/home", "destination": "/" }
    ],
    "version": 2,
    "builds": [
      { "src": "ratemyalttext/src/ratingmyalttext.html", "use": "@vercel/static" },
      { "src": "ratemyalttext/src/ratingmyalttext.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/ratemyalttext", "dest": "/ratemyalttext/src/ratingmyalttext.html" }
    ]
}
