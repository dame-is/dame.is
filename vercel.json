{
    "buildCommand": "npm run build",
    "outputDirectory": "_site",
    "rewrites": [
      { "source": "/writing/blogs", "destination": "/writing/blogs/" },
      { "source": "/writing/posts", "destination": "/writing/posts/" },
      { "source": "/sharing/beliefs", "destination": "/sharing/beliefs/" },
      { "source": "/logging", "destination": "/logging/" },
      { "source": "/supported", "destination": "/supported/" },
      { "source": "/index", "destination": "/" },
      { "source": "/home", "destination": "/" },
      { "source": "/:path*", "destination": "https://publish.obsidian.md/serve?url=dame.is/:path*" }
    ],
    "redirects": [
      {
        "source": "/about",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/blog",
        "destination": "/writing/blogs",
        "permanent": true
      },
      {
        "source": "/posts",
        "destination": "/writing/posts",
        "permanent": true
      },
      {
        "source": "/log",
        "destination": "/logging",
        "permanent": true
      },
      {
        "source": "/patrons",
        "destination": "/supported",
        "permanent": true
      },
      {
        "source": "/ethos",
        "destination": "/sharing/beliefs",
        "permanent": true
      },
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "skeet.tools"
          }
        ],
        "destination": "https://cred.blue/resources",
        "permanent": true
      },
      {
        "source": "/ratingalttext",
        "destination": "https://cred.blue/alt-text",
        "permanent": true
      },
      {
        "source": "/skeet-tools",
        "destination": "https://cred.blue/resources",
        "permanent": true
      },
      {
        "source": "/chip-taste-test",
        "destination": "/eating-chips",
        "permanent": true
      }
    ]
  }